// Test for reaching definition and used-before-definition variables.
//---------------------------------------------------------
 
int bar() {
    int a = 0;
    int b = 1;
    int c = 2;
    if (a > b) {
        a = 2;
        b = 1;
        c = 1;
    } else {
        c = 3;
    }
    return a + b + c;
}
