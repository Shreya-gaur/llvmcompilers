int main()
{
    int x = 1;
    while (x < 11)
    {
        printf("%d", x);
        if (!(x % 2))
        {
            printf(" is a multiple of %d.\n", 2);
            if (!(x % 3))
            {
                printf("%d is a multiple of %d.\n", x, 3);
            }
            if (!(x % 5))
            {
                printf("%d is a multiple of %d.\n", x, 5);
            }
        }
        else if (!(x % 3))
        {
            printf(" is a multiple of %d.\n", 3);
        }
        else if (!(x % 5))
        {
            printf(" is a multiple of %d.\n", 5);
        }
        else
        {
            printf(" is not a multiple of 2, 3 or 5.\n");
        }
        ++x;
    }
    return 0;
}
