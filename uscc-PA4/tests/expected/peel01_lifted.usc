int main()
{
    int x = 9;
    int y = 20;
    int z;
    if (x < y)
    {
        printf("x < y\n");
    }
    if (x < 10)
    {
        z = x + 100;
        printf("start!\n");
        x = z + 4;
        z = y + 3;
        printf("y = %d\n", y);
        ++x;
        while (x < 10)
        {
            x = z + 4;
            z = y + 3;
            printf("y = %d\n", y);
            ++x;
        }
    }
    else
    {
        z = x + 100;
        printf("start!\n");
    }
    return 0;
}
