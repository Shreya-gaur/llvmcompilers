// peel03.usc
//
// Byounguk: Test case for peeling + lifting + optimization
//
// when peeling is not possible

int main()
{
    int x = 2;
    int y = 9;
    int z;
    if (x < y) {
        printf("x < y\n");
    }
    else {
        z = 100;
    }
    z = x + 100;
    printf("start!\n");
    ++x;
    while (x < 10)
    {
        x = z + 4;
        z = y + 3;
        printf("y = %d\n", y);
        ++x;
    }
    return 0;
}
